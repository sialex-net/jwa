{
	"dependencies": {
		"isbot": "^5.1.32",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router": "^7.9.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@cloudflare/vite-plugin": "^1.14.2",
		"@cloudflare/vitest-pool-workers": "^0.10.7",
		"@playwright/test": "^1.56.1",
		"@react-router/dev": "^7.9.6",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.4",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/parser": "^8.46.4",
		"@vitest/browser": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"eslint": "^9.39.1",
		"eslint-plugin-perfectionist": "^4.15.1",
		"knip": "^5.69.1",
		"lefthook": "^1.13.6",
		"playwright": "^1.56.1",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vitest": "^3.2.4",
		"vitest-browser-react": "^1.0.1",
		"wrangler": "^4.48.0"
	},
	"name": "react-router-7-cloudflare-hono",
	"packageManager": "pnpm@10.16.0",
	"private": true,
	"scripts": {
		"biome:check": "biome check --write --verbose",
		"biome:ci": "biome ci --verbose",
		"build": "react-router build",
		"cf:deploy": "react-router build && wrangler deploy",
		"cf:secrets": "wrangler secret bulk ./.dev.vars.production",
		"cf:typegen": "wrangler types",
		"clean": "DEBUG=eslint:eslint,eslint:eslint-helpers eslint --fix --cache --cache-location ./node_modules/.tmp/eslintcache . && biome check --write --verbose",
		"dev": "react-router dev",
		"lint:eslint": "DEBUG=eslint:eslint,eslint:eslint-helpers eslint --fix --cache --cache-location ./node_modules/.tmp/eslintcache .",
		"lint:knip": "knip",
		"preview": "react-router build && vite preview",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:e2e:dev": "playwright test",
		"test:ui": "vitest --ui",
		"typecheck": "wrangler types && react-router typegen && tsc -b"
	},
	"type": "module",
	"version": "1.0.0"
}
